	<requires> <timer/> </requires>

	<events>
		<timeout meaning="failed" sec="20" />
		<log meaning="succeeded">--- finished test-weak_ptr ---</log>
		<log meaning="failed">Error: </log>
	</events>

	<content>
		<rom label="ld.lib.so"/>
		<rom label="test-weak_ptr"/>
	</content>

	<config>
		<parent-provides>
			<service name="ROM"/>
			<service name="LOG"/>
			<service name="CPU"/>
			<service name="RM"/>
			<service name="PD"/>
			<service name="IRQ"/>
			<service name="IO_MEM"/>
			<service name="IO_PORT"/>
			<service name="Timer"/>
		</parent-provides>
		<default-route>
			<any-service> <any-child/> <parent/> </any-service>
		</default-route>
		<default caps="100"/>
		<start name="test-weak_ptr">
			<resource name="RAM" quantum="10M"/>
		</start>
	</config>
