	<requires> <timer/> </requires>

	<events>
		<timeout meaning="failed" sec="50" />
		<log     meaning="succeeded">Test done</log>
	</events>

	<content>
		<rom label="ld.lib.so"/>
		<rom label="test-slab"/>
	</content>

	<config>
		<parent-provides>
			<service name="ROM"/>
			<service name="CPU"/>
			<service name="RM"/>
			<service name="PD"/>
			<service name="IRQ"/>
			<service name="IO_PORT"/>
			<service name="IO_MEM"/>
			<service name="LOG"/>
			<service name="Timer"/>
		</parent-provides>
		<default-route>
			<any-service> <parent/> <any-child/> </any-service>
		</default-route>
		<start name="test-slab" caps="200">
			<resource name="RAM" quantum="64M"/>
		</start>
	</config>
</runtime>
